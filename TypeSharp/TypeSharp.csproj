<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.5.1</Version>
    <Authors>zmjack</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <Description>C# model to the TypeScript model compiler.</Description>
    <Copyright>Copyright © typesharp.net 2018-2019</Copyright>
    <RepositoryUrl>https://github.com/zmjack/TypeSharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/zmjack/TypeSharp</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="JSend" Version="0.0.1" />
    <PackageReference Include="NStandard" Version="0.2.11" />
  </ItemGroup>

</Project>
